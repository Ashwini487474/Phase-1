package pgm.inheritance;

public class product 
{
	int id = 78;
	String name = "Amul";
	public static void display()
	{
		System.out.println("this is display of product class");
	}
	
	
class A extends product
{
	int count = 50;
	String category = "butter";
	public void view()
	{
		System.out.println("this is view of A class");
	}
		
}
class B extends product
{
	int count = 90;
	String category = "Milk";
	public void Show()
	{
		System.out.println("this is Show of B class" )
	}
		
}
